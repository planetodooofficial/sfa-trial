<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="portal_my_expense" name="My Expenses" customize_show="True" inherit_id="portal.portal_my_home">
        <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
            <t t-call="portal.portal_docs_entry">
                <t t-set="title">Expenses</t>
                <t t-set="url" t-value="'/my/expense'"/>
                <t t-set="placeholder_count" t-value="'count_expense'"/>
            </t>
        </xpath>
    </template>
</odoo>